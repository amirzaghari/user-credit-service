{
    "name": "root/user-credit-service",
    "description": "A backend service for managing users and transactions",
    "type": "project",
    "require": {
        "fakerphp/faker": "^1.20",
        "symfony/console": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
